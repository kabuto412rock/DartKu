{
	"name": "dartku",
	"displayName": "DartKu",
	"description": "Generate simply Constructor&Override for Dart",
	"version": "0.6.0",
	"publisher": "LazyLearn",
	"engines": {
		"vscode": "^1.42.0"
	},
	"categories": [
		"Other"
	],
	"repository": {
		"type": "git",
		"url": "git://github.com/kabuto412rock/DartKu.git"
	  },
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "DartKu.generateConstructor",
				"title": "DartKu: Generate Constructor"
			},
			{
				"command": "DartKu.showSuperClassIWant",
				"title": "DartKu: Show Super classðŸ˜Š"
			},
			{
				"command": "DartKu.generateFinalVariableInput",
				"title": "DartKu: Generate Final Varaible from input"
			},
			{
				"command": "DartKu.quickPickDartKu",
				"title": "DartKu: quick pick"
			}
		],
		"menus": {
			"editor/context": [
				{
					"when": "resourceLangId == dart",
					"command": "DartKu.quickPickDartKu",
					"group": "DartKu"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.1",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.42.0",
		"eslint": "^6.8.0",
		"@typescript-eslint/parser": "^2.18.0",
		"@typescript-eslint/eslint-plugin": "^2.18.0",
		"glob": "^7.1.6",
		"mocha": "^7.0.1",
		"typescript": "^3.7.5",
		"vscode-test": "^1.3.0"
	}
}
